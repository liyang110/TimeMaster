            android:text="@string/page7_getaddress"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="@dimen/pagecomponent_text_size"
            layout:new_line="true"
            layout:width_multi="2" />

        <com.time.master.view.BasicEditText
            android:id="@+id/postadd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setbar_backgroundp"
            android:textIsSelectable="true"
            layout:new_line="false"
            layout:width_multi="3" />

        <com.time.master.view.BasicTextView
            android:id="@+id/connect"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#d7d7d7"
            android:gravity="center"
            android:text="@string/page7_phone"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="@dimen/pagecomponent_text_size"
            layout:new_line="true"
            layout:width_multi="2" />

        <com.time.master.view.BasicEditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setbar_backgroundp"
            android:textIsSelectable="true"
            layout:new_line="false"
            layout:width_multi="3" />

        <com.time.master.view.BasicTextView
            android:id="@+id/QQ"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#d7d7d7"
            android:gravity="center"
            android:text="@string/page7_qqnumber"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="@dimen/pagecomponent_text_size"
            layout:new_line="true"
            layout:width_multi="2" />

        <com.time.master.view.BasicEditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setbar_backgroundp"
            android:textIsSelectable="true"
            layout:new_line="false"
            layout:width_multi="3" />

        <com.time.master.view.BasicTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#d7d7d7"
            android:gravity="center"
            android:text="@string/page7_microblog"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="@dimen/pagecomponent_text_size"
            layout:new_line="true"
            layout:width_multi="2" />

        <com.time.master.view.BasicEditText
            android:id="@+id/wb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setbar_backgroundp"
            android:textIsSelectable="true"
            layout:new_line="false"
            layout:width_multi="3" />

        <com.time.master.view.BasicTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#d7d7d7"
            android:gravity="center"
            android:text="@string/page7_message"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textSize="@dimen/pagecomponent_text_size"
            layout:new_line="true"
            layout:width_multi="2" />

        <com.time.master.view.BasicEditText
            android:id="@+id/wx"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setbar_backgroundp"
            android:textIsSelectable="true"
            layout:new_line="false"
            layout:width_multi="3" />
        
       <com.time.master.view.BasicTextView
            android:id="@+id/page7_btnend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#ADD7E7"
            android:gravity="center"
            android:text="@string/page7_end"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#48839F"
            android:textSize="@dimen/pagecomponent_text_size"
            layout:new_line="true"
            layout:width_multi="5" />
    </com.time.master.view.BasicViewGroup>
