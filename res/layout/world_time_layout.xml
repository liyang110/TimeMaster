            android:background="#92D050"
            android:gravity="center"
            android:text="新加\n坡"
            android:textColor="#000000"
            layout:new_line="false"
            layout:width_multi="1" />
    </com.time.master.view.BasicViewGroup>

    <com.time.master.view.BasicViewGroup
        xmlns:layout="http://schemas.android.com/apk/res/com.time.master"
        android:id="@+id/thirdworldtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#BFBFBF" >

        <com.time.master.view.SelectedTextView
            android:id="@+id/solarcalendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#99CCFF"
            android:gravity="center"
            android:text="阳历"
            android:textColor="#000000"
            layout:new_line="false"
            layout:width_multi="1" />

        <com.time.master.view.SelectedTextView
            android:id="@+id/lunarcalendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#99CCFF"
