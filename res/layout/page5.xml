<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/titlesix"
        android:layout_width="fill_parent"
        android:layout_height="27dp"
        android:background="#319EDE"
        android:gravity="center"
        android:paddingBottom="6dp"
        android:paddingTop="6dp"
        android:text="@string/page_title"
        android:textColor="#FFFFFF"
        android:textSize="@dimen/pagetitle_text_size" />

    <TextView
        android:id="@+id/pagetitlesix"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:background="#F0F0F0"
        android:gravity="center"
        android:text="@string/page5_title"
        android:textColor="#000000"
        android:textSize="@dimen/pagecomponent_text_size" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="8dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/idcard"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_idcard"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/idcardtv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:hint="@string/page5_hintid"
                android:textColor="#E0DFDF"
                android:textIsSelectable="true"
                android:textSize="@dimen/pagecomponent_text_size" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/name"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_truename"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/familynametv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="1"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:hint="@string/page5_hintfirst"
                android:textColor="#E0DFDF"
                android:textIsSelectable="true"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/nametv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:hint="@string/page5_hintend"
                android:textColor="#E0DFDF"
                android:textIsSelectable="true"
                android:textSize="@dimen/pagecomponent_text_size" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/username"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_username"
                android:textIsSelectable="true"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/usernametv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/birthday"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_birthday"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/birthdaytv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/birthplace"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_nativeplace"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/birthplacetv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/email"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_email"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/emailtv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/telnum"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_phone"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/telnumtv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/hidnumber"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_code"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/hidnumbertv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/phonecode"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_marginRight="8dp"
                android:layout_weight="2"
                android:background="@drawable/setbar_background"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                android:paddingTop="5dp"
                android:text="@string/page5_getcode"
                android:textSize="@dimen/pagecomponent_text_size" />

            <TextView
                android:id="@+id/phonecodetv"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:background="@drawable/setbar_backgroundp"
                android:gravity="center"
                android:textIsSelectable="true" />
        </LinearLayout>

        <Button
            android:id="@+id/page5_btnend"
            android:layout_width="fill_parent"
            android:layout_height="0dip"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:background="#ADD7E7"
            android:gravity="center"
            android:text="确认"
            android:textColor="#48839F"
            android:textSize="@dimen/pagecomponent_text_size" />
    </LinearLayout>

</LinearLayout>